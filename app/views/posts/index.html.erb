<% @posts.each do |post| %>
  <div class="posts">
  <p><%= post.messages %></p>
  <p><%= post.user.email %></p>
  <a href=<%= "/posts/#{post.id}" %>><%= image_tag post.image.url(:medium) %></a>


  </div>
<% end %>

<h1>This is where you create a new post</h1>

<%= link_to new_post_path do %>
  New post
<% end %>
